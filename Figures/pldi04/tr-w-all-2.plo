#proc yaxis
  stubs: inc 10
  stubformat: %3.0f%%
  minortics: yes
  minorticinc: 5
  grid: color=gray(0.9)
  label: Percentage of Transactional Writes\nto objects larger than given size
  labeldetails: adjust=-.3

#proc xaxis
  selflocatingstubs: text
