#!/bin/sh

SCR_DIR=`dirname $0`

source ${SCR_DIR}/elim_common

BUILD_OUT_DIR=${CALL_DIR}/none/${name}.none

rm -rf ${BUILD_OUT_DIR} 
mkdir ${BUILD_OUT_DIR}

echo "pwd="`pwd`
echo "HARPOON_OPTIONS="${HARPOON_OPTIONS}

java -mx128m -oss100m \
    ${HARPOON_OPTIONS} \
harpoon.Main.SAMain \
    -c $cls -bprecisec -o ${BUILD_OUT_DIR}

exit $?
